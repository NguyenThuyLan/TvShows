{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw"
      },
      "Hosting": {
        "Debug": true
      },
      "RuntimeMinification": {
        "UseInMemoryCache": true,
        "CacheBuster": "Timestamp"
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto",
        "AcceptUnsafeModelsDirectory": true,
        "ModelsDirectory": "~/../TvShows.Web/PublishedModels"
      }
    }
  },
  "AppSettings": {
    "TvMazeUrl": "https://api.tvmaze.com/shows?page=",
    "IsLovalEnv":  true
  },
  //"ConnectionStrings": {
  //  "umbracoDbDSN": "server=LANTHUY\\SQLEXPRESS;database=TvShowTest01;user id=tvShowAdmin;password='1234567890';TrustServerCertificate=true"
  //}
}
